<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="teamA.travel"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="teamA.travel.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="teamA.travel.HotelActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="teamA.travel.RestaurantActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="teamA.travel.InformationActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="teamA.travel.HospitalActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="teamA.travel.SupermarketActivity"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>
